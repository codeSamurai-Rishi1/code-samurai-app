<!DOCTYPE html>
<html>
<head> <% include ./head %> </head>
<body>
<% include ./header %>

    <a class="btn" href="/additem">Add New Item</a>

    <div class="card">

    <table class="highlight">
        <tr>
            <th>Song</th><th>Singer</th><th>Genre</th><th>Price</th>
        </tr>

        <% items.forEach(function(item){  %>
        <tr>
        <td><a href="<%= item.YoutubeURL %>" target="opensim"><%= item.song %></a></td>
        <td><%= item.Singer %></td>
        <td><%= item.genre %></td>
        <td><%= item.Price %></td>
        </tr>

        <% }) %>
    </table>

    </div>

<% include ./footer %>
</body>
</html>
